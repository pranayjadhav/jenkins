<apex:page standardStylesheets="false" sidebar="false" showChat="false" showHeader="false" cache="false" controller="WeworkEngine">
	<apex:stylesheet value="{!URLFOR($Resource.testframeworkresource,'/vendor_css/fontawesome/css/font-awesome.min.css')}"/>
   	<apex:stylesheet value="{!URLFOR($Resource.testframeworkresource,'/vendor_css/bootstrap/css/bootstrap.min.css')}"/>
   	<apex:includeScript value="{!URLFOR($Resource.testframeworkresource,'/vendor_js/jquery/jquery.js')}"/>
   	<apex:includeScript value="{!URLFOR($Resource.testframeworkresource,'/vendor_js/angular/angular.js')}"/>
    <head>
        <title>Automated Test Runner</title>
        <script>
            window.sessionId = '{!$Api.Session_ID}';
            window.currentURL = '{!currentSandboxPath}';
            if('{!$CurrentPage.parameters.local}' == 'true')
                window.resourcePath = 'https://localhost/testframeworkresource';
            else
                window.resourcePath = currentURL + '{!URLFOR($Resource.testframeworkresource)}';
            
            currentURL = currentURL.replace('visual.force', 'salesforce');
            if(currentURL.includes('//c.'))
                currentURL = currentURL.replace('//c.', '//');    
        </script>
    </head>
    <body>
        <div id="wrapper" ng-app="automatedTestRunnerApp" ng-controller="automatedTestRunnerController">
            <nav class="navbar navbar-default navbar-fixed-top">
                  <div class="container">
                  <div class="navbar-header">
                    <a class="jasmine-header" href="#" ></a>
                  	<a class="navbar-brand" href="#">Automated Test Runner</a>
                  </div>
                  </div>
            </nav>
            <div class="container" Id="container-block">
            </div>
        </div>
    </body>
    <style>
            .field-lbl {
                display: block;
                font-weight: bold;
            }
            #container-block{
                  margin-top: 60px;
            }
            pre#json-renderer {
              border: 1px solid #aaa;
              padding: 0.5em 1.5em;
            }
            .jasmine-header { background: url('data:image/png;base64,') no-repeat; background: url('data:image/svg+xml;base64,') no-repeat, none; -moz-background-size: 100%; -o-background-size: 100%; -webkit-background-size: 100%; background-size: 100%; display: block; float: left; width: 110px; height: 25px; margin-top: 10px; }
            @media (min-width: 1200px)
                #container-block,.container {
                    width: auto;
                }
            }
            
        </style>
        <apex:variable value="" var="" rendered="{!$CurrentPage.parameters.local == 'true'}" >
            <link href="https://localhost/testframeworkresource/vendor_css/jasmine/jasmine_favicon.png" type="image/png" rel="shortcut icon" />
            <link href="https://localhost/testframeworkresource/vendor_css/jasmine/jasmine.css" type="text/css" rel="stylesheet" />
            <script lang="javascript" src="https://localhost/testframeworkresource/vendor_css/jasmine/jasmine.js"></script>
            <script lang="javascript" src="https://localhost/testframeworkresource/vendor_css/jasmine/jasmine-html.js"></script>
            <script lang="javascript" src="https://localhost/testframeworkresource/vendor_css/jasmine/jasmine.async.min.js"></script>
        	<script lang="javascript" src="https://localhost/testframeworkresource/vendor_css/jasmine/boot.js"></script>
            <script lang="javascript" src="https://localhost/testframeworkresource/vendor_js/angular/angular-mocks.js"></script>
            <script lang="javascript" src="https://localhost/testframeworkresource/application_js/restapiController.js"></script>
            <script src="/soap/ajax/10.0/connection.js"> </script >
                  <script src="/soap/ajax/10.0/apex.js"> </script >
            <script lang="javascript" src="https://localhost/testframeworkresource/vendor_js/angular/ace.js"></script>
            <script lang="javascript" src="https://localhost/testframeworkresource/vendor_js/angular/ui-ace.min.js"></script>
            <script lang="javascript" src="https://localhost/testframeworkresource/application_js/automatedTestRunner.js"></script>
        </apex:variable>
        <apex:variable value="" var="" rendered="{!$CurrentPage.parameters.local != 'true'}" >
            <apex:stylesheet value="{!URLFOR($Resource.testframeworkresource,'/vendor_css/jasmine/jasmine_favicon.png')}" />
            <apex:stylesheet value="{!URLFOR($Resource.testframeworkresource,'/vendor_css/jasmine/jasmine.css')}" />
            <apex:includeScript value="{!URLFOR($Resource.testframeworkresource,'/vendor_css/jasmine/jasmine.js')}" />
            <apex:includeScript value="{!URLFOR($Resource.testframeworkresource,'/vendor_css/jasmine/jasmine-html.js')}" />
            <apex:includeScript value="{!URLFOR($Resource.testframeworkresource,'/vendor_css/jasmine/jasmine.async.min.js')}"/>
            <apex:includeScript value="{!URLFOR($Resource.testframeworkresource,'/vendor_css/jasmine/boot.js')}" />
            <apex:includeScript value="{!URLFOR($Resource.testframeworkresource,'/vendor_js/angular/angular-mocks.js')}" />
            <apex:includeScript value="{!URLFOR($Resource.testframeworkresource,'/application_js/restapiController.js')}" />
            <script src="/soap/ajax/10.0/connection.js"> </script >
            <script src="/soap/ajax/10.0/apex.js"> </script >
            <apex:includeScript value="{!URLFOR($Resource.testframeworkresource,'/vendor_js/angular/ace.js')}"/>
            <apex:includeScript value="{!URLFOR($Resource.testframeworkresource,'/vendor_js/angular/ui-ace.min.js')}"/>
            <apex:includeScript value="{!URLFOR($Resource.testframeworkresource,'/application_js/automatedTestRunner.js')}"/>
        </apex:variable>
</apex:page>